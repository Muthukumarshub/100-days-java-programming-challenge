#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
    vector<int> nums(n);
    for(int i=0;i<n;i++){
        cin>>nums[i];
    }
    vector<int> res;
    for(int i=0;i<n;i++){   
        int s=nums[i];
        for(int j=1;j=1000;j++){
            if((s+(s*j))==4000){
                    res.push_back(j);
            }
            else{
                continue;
            }
        }
    }
    for(int i=0;i<n;i++){
        cout<<res[i]<<" ";
    }
    return 0;
}

resolve the error
